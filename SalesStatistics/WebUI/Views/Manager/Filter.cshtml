@model Application.UseCases.Managers.Queries.GetFilteringManagersWithPagination.GetFilteringManagersWithPaginationQuery

@using (Html.BeginForm("FilteringManagersPage", "Manager", FormMethod.Post))
{
    @Html.AntiForgeryToken()

    <th scope="col">@Html.EditorFor(m => m.LastNameFilter, 
                        new { htmlAttributes = new { @class = "form-control mr-sm-2", placeholder = "Last name" } })</th>
    <th scope="col"><input type="submit" value="Search" class="btn btn-dark" /></th>
}